{
  "name": "pharmacy-scheduling-system",
  "version": "2.0.0",
  "description": "A modern pharmacy scheduling and communication system built with Node.js, React, and PostgreSQL. Features employee management, SMS integration, real-time updates, and AI-powered assistance.",
  "main": "backend/server.js",
  "scripts": {
    "start": "./start-new-architecture.sh",
    "dev": "docker compose up -d",
    "stop": "docker compose down",
    "restart": "docker compose restart",
    "reset": "docker compose down -v && docker compose up -d",
    "logs": "docker compose logs -f",
    "backend:logs": "docker compose logs -f backend",
    "frontend:logs": "docker compose logs -f frontend",
    "health": "curl -s http://localhost:3001/api/health | jq",
    "data:import": "node scripts/data-processing/import-complete-dataset.js",
    "frontend:dev": "cd frontend && npm run dev",
    "frontend:build": "cd frontend && npm run build",
    "frontend:test": "cd frontend && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheJimmerJammer123/pharmacy-scheduling-system.git"
  },
  "keywords": [
    "pharmacy",
    "scheduling",
    "healthcare",
    "sms",
    "ai-chatbot",
    "nodejs",
    "express",
    "postgresql",
    "react",
    "typescript",
    "socket-io"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TheJimmerJammer123/pharmacy-scheduling-system/issues"
  },
  "homepage": "https://github.com/TheJimmerJammer123/pharmacy-scheduling-system#readme",
  "dependencies": {
    "xlsx": "^0.18.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
